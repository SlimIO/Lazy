{
  "name": "@slimio/lazy",
  "version": "1.2.0",
  "description": "SlimIO Lazy Properties lib",
  "main": "index.js",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "prepublishOnly": "pkg-ok",
    "test": "cross-env psp && ava --verbose",
    "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=html"
  },
  "husky": {
    "hooks": {
      "pre-push": "cross-env eslint index.js && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 98,
    "statements": 98,
    "functions": 98,
    "branches": 90
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SlimIO/Lazy.git"
  },
  "keywords": [
    "lazy",
    "evaluation",
    "property",
    "object",
    "getter",
    "setter"
  ],
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "author": "SlimIO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SlimIO/Lazy/issues"
  },
  "homepage": "https://github.com/SlimIO/Lazy#readme",
  "dependencies": {
    "@slimio/is": "^1.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@escommunity/minami": "^1.0.0",
    "@slimio/eslint-config": "^3.0.3",
    "@slimio/psp": "^0.9.2",
    "ava": "^3.5.2",
    "cross-env": "^6.0.3",
    "eslint": "^6.6.0",
    "husky": "^3.0.9",
    "jsdoc": "^3.5.5",
    "nyc": "^14.0.0",
    "pkg-ok": "^2.3.1"
  }
}
